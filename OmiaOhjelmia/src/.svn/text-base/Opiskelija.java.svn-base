
public class Opiskelija {

    private String nimi;
    private String Numero;

    public Opiskelija(String n, String nro) {
        this.nimi = n;
        this.Numero = nro;
    }

    public String getNimi() {
        return nimi;
    }

    public String getNro() {
        return Numero;
    }

    public String toString() {
        return nimi + " (" + Numero + ")";
    }
}
